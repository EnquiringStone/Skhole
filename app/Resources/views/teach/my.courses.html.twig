{% extends 'base.html.twig' %}
{% block title %}{% trans %}t.my_courses{% endtrans %}{% endblock %}
{% block prescripts %}
    <script src="{{ asset('js/my-courses/my.courses.js') }}"></script>
{% endblock %}
{% block body %}
    {% include ':panels/my-courses:my.courses.panel.html.twig' %}
{% endblock %}
{% block keywords %}{% trans %}meta.description_keywords.create_my_courses{% endtrans %}{% endblock %}
{% block description %}{% trans %}meta.description_content.create_my_courses{% endtrans %}{% endblock %}
{% block postscripts %}
    <script>
        $(document).ready(function() {
            sendAjaxCall('{{url("app_home_ajax_page")}}', {'method': 'getReviewModals', 'ajax_key': 'CSAS1', 'offset': {{ offset }}, 'limit': {{ limit }}, 'sortAttribute': 'id', 'sortValue': 'ASC', 'context': 'SELF'},
                    function(args) {
                        $('.review-modals').append(args['html']);
                    }, function(message) {

                    });
        });
    </script>
{% endblock %}

{% set menu = 'teach' %}
{% set subMenu = 'myCourses' %}