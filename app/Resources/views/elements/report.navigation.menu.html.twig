{% block reportNavigationMenu %}
    <div class="centrum">
        <div class="breadcrumb">
            <div class="label label-primary">
                <button type="button" class="btn btn-primary btn-xs unclickable">
                    <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                </button>
                {% if viewType == 'teach' %}
                    <a href=" {{ path('app_teach_progression_course_members') }}" class="btn btn-default btn-sm">
                        <span class="glyphicon glyphicon-remove text-danger" aria-hidden="true"></span>
                        {% trans %}o.close_course{% endtrans %}
                    </a>
                {% elseif viewType == 'learn' %}
                    <a href=" {{ path('app_learn_course_reports') }}" class="btn btn-default btn-sm">
                        <span class="glyphicon glyphicon-remove text-danger" aria-hidden="true"></span>
                        {% trans %}o.close_course{% endtrans %}
                    </a>
                {% endif %}

                <button type="button" class="btn btn-primary btn-xs unclickable">
                    <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                </button>
            </div>
        </div>
    </div>
{% endblock %}